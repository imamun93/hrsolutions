   "source": [
    "def jumpingOnClouds(c):\n",
    "    jump, i= 0, 0\n",
    "    while i < len(c)-1:\n",
    "        if i+2 < len(c) and c[i+2] !=1:\n",
    "            i +=1\n",
    "        jump +=1\n",
    "        i+=1\n",
    "    return jump"
   ]
